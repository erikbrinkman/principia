{
  "name": "principia",
  "version": "0.0.25",
  "description": "A library for making plots based off of principae",
  "bin": "bin/cmd.js",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "format": "prettier --write bin/cmd.js src/*.js test/*.js resources/*.js",
    "lint": "eslint bin/cmd.js src/*.js resources/post_script.js resources/pre_script.js test/*.js",
    "todo": "grep -nrEI 'FIXME|TODO|XXX' bin src test resources"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikbrinkman/principia.git"
  },
  "keywords": [
    "plot"
  ],
  "author": "Erik Brinkman <erik.brinkman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikbrinkman/principia/issues"
  },
  "homepage": "https://github.com/erikbrinkman/principia#readme",
  "dependencies": {
    "chrome-launcher": "^0.10.5",
    "chrome-remote-interface": "^0.27.0",
    "clean-css": "^4.2.1",
    "is-wsl": "^1.1.0",
    "principia-plot": "0.0.11",
    "tempy": "^0.2.1",
    "uglify-es": "^3.3.9",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "jest": "^24.0.0",
    "prettier": "^1.16.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "testEnvironment": "node"
  }
}
