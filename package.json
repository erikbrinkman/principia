{
  "name": "principia",
  "version": "0.0.7",
  "description": "A library for making plots loosely based off of principae",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "docs": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikbrinkman/principia.git"
  },
  "keywords": [
    "plot"
  ],
  "author": "Erik Brinkman <erik.brinkman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikbrinkman/principia/issues"
  },
  "homepage": "https://github.com/erikbrinkman/principia#readme",
  "dependencies": {
    "@turf/turf": "^4.7.3",
    "d3": "^4.11.1",
    "fmin": "0.0.2",
    "quadprog": "^1.5.1"
  },
  "devDependencies": {
    "@types/d3": "^4.11.1",
    "@types/geojson": "^1.0.6",
    "@types/node": "^8.0.53",
    "headlesspdf": "0.0.8",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
